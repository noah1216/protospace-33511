<div class="card">
  <%=  image_tag prototype.image, class: :card__img if prototype.image.attached? %>
  <div class="card__body">
    <%= link_to prototype.title, prototype_path(prototype.id), class: :card__title%>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to prototype.concept, prototype_path(prototype.id), class: :card__user %>
  </div>
</div>